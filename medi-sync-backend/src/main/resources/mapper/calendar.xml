<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mediSync.project.common.mapper.CalendarMapper">

    <select id="getReservation" parameterType="Integer" resultType="reservation">
        select * from reservation where patient_id = #{patient_id}
    </select>

    <select id="getTestReservation" parameterType="Integer" resultType="testReservation">
        select * from test_reservation where patient_id = #{patient_id}
    </select>

    <select id="getTestSchedule" parameterType="Integer" resultType="testSchedule">
        select * from test_schedule where schedule_id = #{schedule_id}
    </select>

    <select id="getOperation" parameterType="Integer" resultType="operation">
        select * from operation where patient_id = #{patient_id}
    </select>



</mapper>
